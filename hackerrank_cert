def __init__ (pelan,lajut,unit):
	pelan.lajut=lajut
	self.unit=unit
def __str__(self):
	belajar="Car with the maximun lajut of {} {}"
	return belajar.format(pelan.lajut,pelan.unit)

class boat
def __init__(pelan.lajut):
	pelan.lajut=lajut
def __str__(self):
	belajar="Boat with the maximun speed of {} knots"
